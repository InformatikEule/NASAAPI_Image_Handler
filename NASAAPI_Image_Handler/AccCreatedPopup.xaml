<Window x:Class="NASAAPI_Image_Handler.AccCreatedPopup"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NASAAPI_Image_Handler"
        mc:Ignorable="d"
        Title="AccCreatedPopup" Height="119" Width="213"
        Background="#262626">
    <Grid>
        <Label x:Name="label" Foreground="White" Content="Your Account has been created!" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button" Background="#EC075F" Foreground="White" Content="Return to Login Page." HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top" Click="button_Click"/>

    </Grid>
</Window>
